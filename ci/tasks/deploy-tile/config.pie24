product-name: mongodb-on-demand
product-properties:
  .properties.az_multi_select:
    value:
    - us-central1-f
  .properties.backup_enabled:
    value: false
  .properties.bosh_dns_disable:
    value: false
  .properties.org:
    value: mongodb-on-demand-org
  .properties.space:
    value: mongodb-on-demand-space
  .properties.ssl_enabled:
    value: false
  .properties.syslog_tls_enabled:
    value: false
  .properties.syslog_transport_protocol:
    value: tcp
  .properties.url:
    value: http://ec2-3-223-45-28.compute-1.amazonaws.com:9080
  .properties.username:
    value: ((OM_API_USER))
  .properties.api_key:
    value:
      secret:  ((OM_API_KEY))
  .properties.vm_extensions:
    value:
    - public_ip
network-properties:
  network:
    name: patterson-pas-subnet
  other_availability_zones:
  - name: "us-central1-f"
  service_network:
    name: patterson-services-subnet
  singleton_availability_zone:
    name: "us-central1-f"
resource-config:
  broker:
    instances: automatic
    persistent_disk:
      size_mb: automatic
    instance_type:
      id: automatic
    internet_connected: true
errand-config:
  delete-all-service-instances:
    pre-delete-state: false
  deregister-broker:
    pre-delete-state: false
  register-broker:
    post-deploy-state: true
  upgrade-all-service-instances:
    post-deploy-state: false
